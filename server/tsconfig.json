{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "module": "commonjs",
    "target": "es5",
    "rootDir": "./",
    "baseUrl": "./src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "downlevelIteration": true
  },
  "include": ["./src/**/*", "./security/*", "./tests/**/*"]
}
